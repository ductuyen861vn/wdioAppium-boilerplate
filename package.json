{
  "name": "thread-participant-app",
  "version": "0.0.1",
  "author": "Thread",
  "license": "Thread",
  "description": "Thread project to run WebdriverIO tests with Appium to test native applications on iOS and Android",
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "TBD"
  },
  "bugs": {
    "url": "https://github.com/webdriverio/appium-boilerplate/issues"
  },
  "engines": {
    "node": "^16.13.0 || >=18.0.0"
  },
  "keywords": [
    "WebdriverIO",
    "WebDriver",
    "appium",
    "native app",
    "hybrid",
    "android",
    "ios"
  ],
  "type": "module",
  "scripts": {
    "wdioMobileAndroidLocal": "wdio run config/local/wdio.app.mobile.android.local.conf.ts",
    "wdioWeb": "wdio run config/local/wdio.web.destop.local.conf.ts",
    "wdioMobileIOSLocal": "wdio run config/local/wdio.app.mobile.ios.local.conf.ts",
    "test": "wdio run config/local/wdio.web.destop.local.conf.ts & wdio run config/local/wdio.app.mobile.ios.local.conf.ts",
    "bsIOS": "SPECS=../../tests/specs/mobile/ios/**/ios.login.ts wdio run config/browserstack/wdio.app.mobile.ios.bs.conf.ts",
    "demo": "SPECS=../../tests/specs/mobile/ios/**/ios.login.ts wdio run config/browserstack/wdio.app.mobile.ios.bs.conf.ts & wdio run config/browserstack/wdio.app.mobile.android.bs.conf.ts & wdio run config/local/wdio.web.mobile.ios.local.conf.ts",
    "bsMobile": "SPECS=../../tests/specs/mobile/ios/**/ios.login.ts wdio run config/browserstack/wdio.app.mobile.ios.bs.conf.ts & wdio run config/browserstack/wdio.app.mobile.android.bs.conf.ts",
    "bsMobile2": "BS_IOS_VERSION=16 SPECS=../../tests/specs/mobile/ios/**/ios.login.ts wdio run config/browserstack/wdio.app.mobile.ios.bs.conf.ts & SPECS=../../tests/specs/mobile/ios/**/ios.home.ts wdio run config/browserstack/wdio.app.mobile.ios.bs.conf.ts & wdio run config/browserstack/wdio.app.mobile.android.bs.conf.ts",
    "bsWeb": "wdio run config/browserstack/wdio.web.mobile.ios.bs.conf.ts"
  },
  "devDependencies": {
    "@types/chai": "^4.3.16",
    "@types/jsonfile": "^6.1.4",
    "@types/mocha": "^10.0.6",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@wdio/allure-reporter": "^8.36.1",
    "@wdio/appium-service": "^8.36.1",
    "@wdio/browser-runner": "^8.36.1",
    "@wdio/browserstack-service": "^8.36.1",
    "@wdio/cli": "^8.36.1",
    "@wdio/globals": "^8.36.1",
    "@wdio/junit-reporter": "^8.38.0",
    "@wdio/local-runner": "^8.36.1",
    "@wdio/logger": "^8.38.0",
    "@wdio/mocha-framework": "^8.36.1",
    "@wdio/sauce-service": "^8.36.1",
    "@wdio/spec-reporter": "^8.36.1",
    "@wdio/testingbot-service": "^8.36.1",
    "allure-commandline": "^2.29.0",
    "appium": "^2.4.1",
    "appium-chromedriver": "^5.6.54",
    "appium-flutter-driver": "^2.6.0",
    "appium-safari-driver": "^3.5.14",
    "appium-uiautomator2-driver": "latest",
    "appium-webdriveragent": "^8.6.0",
    "appium-xcuitest-driver": "latest",
    "axios": "^1.7.2",
    "chai": "^5.1.1",
    "chromedriver": "^125.0.2",
    "date-fns": "^3.6.0",
    "eslint-plugin-wdio": "^8.24.12",
    "fs-extra": "^11.2.0",
    "i": "^0.3.7",
    "jsonfile": "^6.1.0",
    "module-alias": "^2.2.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5",
    "wdio-chromedriver-service": "^8.0.0"
  }
}
